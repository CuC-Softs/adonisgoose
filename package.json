{
    "name": "adonisgoose",
    "version": "0.0.5",
    "description": "a support to mongoose orm into adonisjs v5",
    "main": "index.js",
    "scripts": {
        "test": "echo testing",
        "clean": "del build",
        "compile": "npm run lint && npm run clean && tsc && npm run copyfiles",
        "copyfiles": "copyfiles \"templates/**/*.txt\" \"instructions.md\" build",
        "lint": "eslint . --ext=.ts",
        "commit": "git-cz"
    },
    "files": [
        "build/commands",
        "build/providers",
        "build/templates"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CuC-Softs/adonisgoose.git"
    },
    "keywords": [
        "mongoose",
        "adonisjs",
        "js",
        "node",
        "javascript",
        "mongodb",
        "mongo",
        "adonisv5",
        "adonis"
    ],
    "publishConfig": {
        "tag": "latest",
        "access": "public"
    },
    "adonisjs": {
        "instructions": "./build/instructions.js",
        "instructionsMd": "./build/instructions.md",
        "types": "adonisgoose",
        "providers": [
            "adonisgoose"
        ],
        "commands": [
            "adonisgoose/build/commands"
        ]
    },
    "author": "manuel-antunes,cucsofts",
    "license": "MIT",
    "devDependencies": {
        "@adonisjs/core": "^5.3.2",
        "@adonisjs/mrm-preset": "^4.1.2",
        "@adonisjs/repl": "^3.1.6",
        "@adonisjs/require-ts": "^2.0.8",
        "@adonisjs/sink": "^5.1.6",
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@poppinss/dev-utils": "^1.1.5",
        "@types/jest": "^27.0.1",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "commitizen": "^4.2.4",
        "copyfiles": "^2.4.1",
        "cz-conventional-changelog": "^3.3.0",
        "del-cli": "^4.0.1",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-adonis": "^1.3.3",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-prettier": "^4.0.0",
        "execa": "^5.1.1",
        "get-port": "^5.1.1",
        "husky": "^7.0.2",
        "japa": "^3.1.1",
        "jest": "^27.1.0",
        "lint-staged": "^11.1.2",
        "prettier": "^2.3.2",
        "ts-jest": "^27.0.5",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.11.0",
        "typescript": "^4.4.2"
    },
    "peerDependencies": {
        "@adonisjs/core": "^5.1.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "directories": {
        "test": "test"
    },
    "dependencies": {
        "@types/mongoose": "^5.11.97",
        "mongoose": "^6.0.4",
        "@poppinss/hooks": "^3.0.5",
        "@poppinss/utils": "^3.2.0"
    }
}
